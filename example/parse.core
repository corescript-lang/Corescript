var string = Hello world
var length = [len string]

var c = 0
var char = [blank]
:loopChar
	set char = [char string c]
	if c = [length]:endLoopChar
	if char = [space]:loopCharSpace

	print [char]

	set c = [add c 1]
goto loopChar

# Go back to loop
:loopCharSpace
	print ---
return loopCharSpace

:endLoopChar
print .
